//FIFO implementation by using array
//gcc arr_fifo.c -o arr_fifo

#include <stdio.h>

#define N 10

//Global variables

int Arr[N];
int front = -1, rear = -1;

//Prototypes
int IsEmpty(void);
int IsFull(void);
void Push(int val);
int Pop(void);

int main(void)
{

	return 0;
}

int IsEmpty(void)
{
	if((front == -1) && (rear == -1))
		return 1;
	if(front == rear)
		return 1;
	return 0;
} 

int IsFull(void)
{
	if((front == 0) && (rear == N - 1))
		return 1;
	if((front - rear) == 1)
		return 1;
	return 0;
} 

void Push(int val)
{

}

int Pop(void)
{
	return 0;
}

